{
  "name": "sikessem.org",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "description": "The Sikessem Platform",
  "keywords": ["sikessem", "platform", "paas", "web-application", "web-services"],
  "engines": {
    "bun": ">=1.1.34",
    "node": "prefer bun instead",
    "npm": "prefer bun instead"
  },
  "packageManager": "bun@1.1.34",
  "scripts": {
    "dev": "vite",
    "check": "biome ci .",
    "check:types": "tsc --incremental --noEmit",
    "fix": "bun fix:format && bun fix:lint",
    "fix:all": "bun fix:unsafe && bun fix",
    "fix:format": "biome format --write .",
    "fix:lint": "biome check --write .",
    "fix:unsafe": "biome check --fix --unsafe",
    "format": "biome format .",
    "lint": "biome check .",
    "prod": "vite build && vite build --ssr"
  },
  "devDependencies": {
    "@alpinejs/focus": "^3.14.3",
    "@biomejs/biome": "^1.9.4",
    "@fontsource-variable/figtree": "^5.1.1",
    "@fontsource/rajdhani": "^5.1.0",
    "@inertiajs/vue3": "^1.2.0",
    "@mdi/js": "^7.4.47",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^0.0.0-development.1",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^0.0.0-development.1",
    "@types/bun": "^1.1.13",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "alpinejs": "^3.14.3",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "laravel-echo": "^1.16.1",
    "laravel-precognition-alpine": "^0.5.12",
    "laravel-precognition-vue-inertia": "^0.5.12",
    "laravel-vite-plugin": "^1.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.2.6",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "pusher-js": "^8.4.0-rc2",
    "resolve-url-loader": "5.0.0",
    "styliz": "^0.0.2",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.6.3",
    "vitest": "^2.1.4",
    "vue": "^3.5.12"
  }
}
