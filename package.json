{
  "name": "sikessem.com",
  "description": "The Sikessem Web Application",
  "private": true,
  "license": "AGPL-3.0-only",
  "type": "module",
  "engines": {
    "node": ">=20.14.0",
    "pnpm": ">=9.1.3",
    "npm": "prefer pnpm instead"
  },
  "packageManager": "pnpm@9.1.3",
  "scripts": {
    "check": "biome ci .",
    "dev": "vite",
    "fix": "pnpm format.fix && pnpm lint.fix",
    "format": "biome format .",
    "format.fix": "biome format --write .",
    "lint": "biome check .",
    "lint.fix": "biome check --apply-unsafe .",
    "prod": "vite build",
    "test": "vitest",
    "test.coverage": "vitest run --coverage",
    "test.run": "vitest run",
    "test.ui": "vitest --ui",
    "test.watch": "vitest watch",
    "type.check": "tsc --incremental --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@fontsource-variable/figtree": "^5.0.21",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.12.13",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "laravel-vite-plugin": "^1.0.4",
    "postcss": "^8.4.38",
    "sass": "^1.77.2",
    "styliz": "^0.0.2",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
